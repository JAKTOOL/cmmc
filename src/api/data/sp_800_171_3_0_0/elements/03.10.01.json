                                        "elementIdentifier": "SR-03.10.01.a",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "a",
                                        "text": "Develop, approve, and maintain a list of individuals with authorized access to the facility where the system resides.",
                                        "elements": [
                                            {
                                                "elementIdentifier": "DS-A.03.10.01.a[01]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "a list of individuals with authorized access to the facility where the system resides is developed."
                                            },
                                            {
                                                "elementIdentifier": "DS-A.03.10.01.a[02]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "a list of individuals with authorized access to the facility where the system resides is approved."
                                            },
                                            {
                                                "elementIdentifier": "DS-A.03.10.01.a[03]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
