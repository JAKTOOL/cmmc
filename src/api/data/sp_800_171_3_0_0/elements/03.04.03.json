                                "elements": [
                                    {
                                        "elementIdentifier": "SR-03.04.03.a",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "a",
                                        "text": "Define the types of changes to the system that are configuration-controlled.",
                                        "elements": [
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.a",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "the types of changes to the system that are configuration-controlled are defined."
                                            }
                                        ]
                                    },
                                    {
                                        "elementIdentifier": "SR-03.04.03.b",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "b",
                                        "text": "Review proposed configuration-controlled changes to the system, and approve or disapprove such changes with explicit consideration for security impacts.",
                                        "elements": [
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.b[01]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "proposed configuration-controlled changes to the system are reviewed with explicit consideration for security impacts."
                                            },
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.b[02]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "proposed configuration-controlled changes to the system are approved or disapproved with explicit consideration for security impacts."
                                            }
                                        ]
                                    },
                                    {
                                        "elementIdentifier": "SR-03.04.03.c",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "c",
                                        "text": "Implement and document approved configuration-controlled changes to the system.",
                                        "elements": [
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.c[01]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "approved configuration-controlled changes to the system are implemented."
                                            },
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.c[02]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "approved configuration-controlled changes to the system are documented."
                                            }
                                        ]
                                    },
                                    {
                                        "elementIdentifier": "SR-03.04.03.d",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "d",
                                        "text": "Monitor and review activities associated with configuration-controlled changes to the system.",
                                        "elements": [
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.d[01]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "activities associated with configuration-controlled changes to the system are monitored."
                                            },
                                            {
                                                "elementIdentifier": "DS-A.03.04.03.d[02]",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "activities associated with configuration-controlled changes to the system are reviewed."
                                            }
                                        ]
                                    }
