                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "a",
                                        "text": "Prevent non-privileged users from executing privileged functions.",
                                        "elements": [
                                            {
                                                "elementIdentifier": "DS-A.03.01.07.a",
                                                "elementTypeIdentifier": "determination",
                                                "title": "",
                                                "text": "non-privileged users are prevented from executing privileged functions."
                                            }
