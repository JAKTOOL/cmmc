                    {
                        "elementIdentifier": "03.06.01",
                        "elementTypeIdentifier": "requirement",
                        "title": "Incident Handling",
                        "text": "",
                        "elements": [
                            {
                                "elementIdentifier": "D-03.06.01",
                                "elementTypeIdentifier": "discussion",
                                "title": "",
                                "text": "Incident-related information can be obtained from a variety of sources, including audit monitoring, network monitoring, physical access monitoring, user and administrator reports, and reported supply chain events. An effective incident handling capability involves coordination among many organizational entities, including mission and business owners, system owners, human resources offices, physical and personnel security offices, legal departments, operations personnel, and procurement offices."
                            },
                            {
                                "elementIdentifier": "E-03.06.01",
                                "elementTypeIdentifier": "examine",
                                "title": "",
                                "text": "[SELECT FROM: incident response policy and procedures; contingency planning policy and procedures; procedures for incident handling; procedures for incident response planning; incident response plan; contingency plan; records of incident response plan reviews and approvals; system security plan; other relevant documents or records]"
                            },
                            {
                                "elementIdentifier": "I-03.06.01",
                                "elementTypeIdentifier": "interview",
                                "title": "",
                                "text": "[SELECT FROM: personnel with incident handling responsibilities; personnel with incident response planning responsibilities; personnel with contingency planning responsibilities; personnel with information security responsibilities]"
                            },
                            {
                                "elementIdentifier": "SP 800-161",
                                "elementTypeIdentifier": "reference",
                                "title": "Boyens JM, Smith A, Bartol N, Winkler K, Holbrook A, Fallon M (2022) Cybersecurity Supply Chain Risk Management Practices for Systems and Organizations. (National Institute of Standards and Technology",
                                "text": "https://doi.org/10.6028/NIST.SP.800-161r1"
                            },
                            {
                                "elementIdentifier": "SP 800-50",
                                "elementTypeIdentifier": "reference",
                                "title": "Wilson M, Hash J (2003) Building an Information Technology Security Awareness and Training Program. (National Institute of Standards and Technology, Gaithersburg, MD), NIST Special Publication (SP) 80",
                                "text": "https://doi.org/10.6028/NIST.SP.800-50"
                            },
                            {
                                "elementIdentifier": "SP 800-61",
                                "elementTypeIdentifier": "reference",
                                "title": "Cichonski PR, Millar T, Grance T, Scarfone KA (2012) Computer Security Incident Handling Guide. (National Institute of Standards and Technology, Gaithersburg, MD), NIST Special Publication (SP) 800-61",
                                "text": "https://doi.org/10.6028/NIST.SP.800-61r2"
                            },
                            {
                                "elementIdentifier": "SR-03.06.01",
                                "elementTypeIdentifier": "security_requirement",
                                "title": "",
                                "text": "Implement an incident-handling capability that is consistent with the incident response plan and includes preparation, detection and analysis, containment, eradication, and recovery.",
                                "elements": [
                                    {
                                        "elementIdentifier": "DS-A.03.06.01[01]",
                                        "elementTypeIdentifier": "determination",
                                        "title": "",
                                        "text": "an incident-handling capability that is consistent with the incident response plan is implemented."
                                    },
                                    {
                                        "elementIdentifier": "DS-A.03.06.01[02]",
                                        "elementTypeIdentifier": "determination",
                                        "title": "",
                                        "text": "the incident handling capability includes preparation."
                                    },
                                    {
                                        "elementIdentifier": "DS-A.03.06.01[03]",
                                        "elementTypeIdentifier": "determination",
                                        "title": "",
                                        "text": "the incident handling capability includes detection and analysis."
                                    },
                                    {
                                        "elementIdentifier": "DS-A.03.06.01[04]",
                                        "elementTypeIdentifier": "determination",
                                        "title": "",
                                        "text": "the incident handling capability includes containment."
                                    },
                                    {
                                        "elementIdentifier": "DS-A.03.06.01[05]",
                                        "elementTypeIdentifier": "determination",
                                        "title": "",
                                        "text": "the incident handling capability includes eradication."
                                    },
                                    {
                                        "elementIdentifier": "DS-A.03.06.01[06]",
                                        "elementTypeIdentifier": "determination",
                                        "title": "",
                                        "text": "the incident handling capability includes recovery."
                                    }
                                ],
