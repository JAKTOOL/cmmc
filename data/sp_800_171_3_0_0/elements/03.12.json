                    {
                        "elementIdentifier": "03.12.02",
                        "elementTypeIdentifier": "requirement",
                        "title": "Plan of Action and Milestones",
                        "text": "",
                        "elements": [
                            {
                                "elementIdentifier": "D-03.12.02",
                                "elementTypeIdentifier": "discussion",
                                "title": "",
                                "text": "Plans of action and milestones (POAMs) are important documents in organizational security programs. Organizations use POAMs to describe how unsatisfied security requirements will be met and how planned mitigations will be implemented. Organizations can document system security plans and POAMs as separate or combined documents in any format. Federal agencies may consider system security plans and POAMs as inputs to risk-based decisions on whether to process, store, or transmit CUI on a system hosted by a nonfederal organization."
                            },
                            {
                                "elementIdentifier": "E-03.12.02",
                                "elementTypeIdentifier": "examine",
                                "title": "",
                                "text": "[SELECT FROM: security assessment and monitoring policy and procedures; procedures for plans of action and milestones; security assessment plan; security assessment report; security assessment evidence; plan of action and milestones; system security plan; other relevant documents or records]"
                            },
                            {
                                "elementIdentifier": "I-03.12.02",
                                "elementTypeIdentifier": "interview",
                                "title": "",
                                "text": "[SELECT FROM: personnel with plans of action and milestones development and implementation responsibilities; personnel with information security responsibilities]"
                            },
