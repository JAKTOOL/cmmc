                                "elementTypeIdentifier": "security_requirement",
                                "title": "",
                                "text": "",
                                "elements": [
                                    {
                                        "elementIdentifier": "SR-03.12.02.a",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "a",
                                        "text": "Develop a plan of action and milestones for the system:",
                                        "elements": [
                                            {
                                                "elementIdentifier": "SR-03.12.02.a.01",
                                                "elementTypeIdentifier": "security_requirement",
                                                "title": "01",
                                                "text": "To document the planned remediation actions to correct weaknesses or deficiencies noted during security assessments and",
                                                "elements": [
                                                    {
                                                        "elementIdentifier": "DS-A.03.12.02.a.01",
                                                        "elementTypeIdentifier": "determination",
                                                        "title": "",
                                                        "text": "a plan of action and milestones for the system is developed to document the planned remediation actions for correcting weaknesses or deficiencies noted during security assessments."
                                                    }
                                                ]
                                            },
                                            {
                                                "elementIdentifier": "SR-03.12.02.a.02",
                                                "elementTypeIdentifier": "security_requirement",
                                                "title": "02",
                                                "text": "To reduce or eliminate known system vulnerabilities.",
                                                "elements": [
                                                    {
                                                        "elementIdentifier": "DS-A.03.12.02.a.02",
                                                        "elementTypeIdentifier": "determination",
                                                        "title": "",
                                                        "text": "a plan of action and milestones for the system is developed to reduce or eliminate known system vulnerabilities."
                                                    }
                                                ]
                                            }
                                        ]
                                    },
                                    {
                                        "elementIdentifier": "SR-03.12.02.b",
                                        "elementTypeIdentifier": "security_requirement",
                                        "title": "b",
                                        "text": "Update the existing plan of action and milestones based on the findings from:",
                                        "elements": [
                                            {
                                                "elementIdentifier": "SR-03.12.02.b.01",
                                                "elementTypeIdentifier": "security_requirement",
                                                "title": "01",
                                                "text": "Security assessments,",
                                                "elements": [
                                                    {
                                                        "elementIdentifier": "DS-A.03.12.02.b.01",
                                                        "elementTypeIdentifier": "determination",
                                                        "title": "",
                                                        "text": "the existing plan of action and milestones is updated based on the findings from security assessments."
                                                    }
                                                ]
                                            },
                                            {
                                                "elementIdentifier": "SR-03.12.02.b.02",
                                                "elementTypeIdentifier": "security_requirement",
                                                "title": "02",
                                                "text": "Audits or reviews, and",
                                                "elements": [
                                                    {
                                                        "elementIdentifier": "DS-A.03.12.02.b.02",
                                                        "elementTypeIdentifier": "determination",
                                                        "title": "",
                                                        "text": "the existing plan of action and milestones is updated based on the findings from audits or reviews."
                                                    }
                                                ]
                                            },
                                            {
                                                "elementIdentifier": "SR-03.12.02.b.03",
                                                "elementTypeIdentifier": "security_requirement",
                                                "title": "03",
                                                "text": "Continuous monitoring activities.",
                                                "elements": [
                                                    {
                                                        "elementIdentifier": "DS-A.03.12.02.b.03",
                                                        "elementTypeIdentifier": "determination",
                                                        "title": "",
                                                        "text": "the existing plan of action and milestones is updated based on the findings from continuous monitoring activities."
